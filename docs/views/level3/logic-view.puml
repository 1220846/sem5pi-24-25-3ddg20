component "Backend Data Management" <<component>>{
    () "DB API" as DBAPI
    portin " " as PI
    portout " " as SARMPI1

    component "Server" <<component>> <<C4 Container>>{
        portin " " as SVPI
        portout " " as SVPO
        portout " " as SVPO1

    }

    component "Database" <<component>> <<C4 Container>>{
        portin " " as DBPI
    }
     
